# {{ ansible_managed }}
# Tor configuration file

# Basic configuration
SocksPort {{ tor_socks_port }}
ControlPort {{ tor_control_port }}

# Logging
Log notice file /var/log/tor/notices.log
Log info file /var/log/tor/info.log

# Set Tor to run as a daemon
RunAsDaemon 1

# Data directory
DataDirectory /var/lib/tor